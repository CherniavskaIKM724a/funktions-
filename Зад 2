#include <iostream>
using namespace std;
long long factorial(int a)
{
    int res=1;
    for(unsigned i{1};i<=a;i++)
        {
            res *= i;
        }
    return res;
}
int main()
{
    long long n1;
    cout<<"Enter number(>=0): ";cin>>n1;
    cout<<"!"<<n1<< " = "<<factorial(n1);
    return 0;
}
